{
  "type": "module",
  "name": "fishdash",
  "version": "0.0.3",
  "description": "Functional utility library for modern TypeScript, ESM, ES6+",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "build": "            bunup",
    "dev": "              bunup --watch",
    "test:node": "        node --import=tsx --test tests/*.test.ts",
    "test:node:watch": "  node --import=tsx --test --watch tests/*.test.ts",
    "test:lint": "        biome check src tests",
    "lint": "             biome lint src tests",
    "format": "           biome format --write src tests",
    "format:check": "     biome format src tests",
    "test": "             npm run test:lint && npm run test:node",
    "test:watch": "       npm run test:node:watch",
    "version:path": "     npm version patch && git push && git push --tags",
    "version:release": "  semantic-release",
    "release": "          npm run build && npm run test && npm run version:release && npm publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.1",
    "@types/node": "^24.9.1",
    "bunup": "^0.15.12",
    "semantic-release": "^25.0.1",
    "tsx": "^4.20.6",
    "typescript": "5.9.3"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "files": [
    "lib",
    "cjs",
    "LICENSE",
    "README.md"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.js",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "1.4kb"
    },
    {
      "path": "lib/any.js",
      "limit": "1kb"
    },
    {
      "path": "lib/count.js",
      "limit": "1kb"
    },
    {
      "path": "lib/countBy.js",
      "limit": "1kb"
    },
    {
      "path": "lib/each.js",
      "limit": "1kb"
    },
    {
      "path": "lib/every.js",
      "limit": "1kb"
    },
    {
      "path": "lib/fromPairs.js",
      "limit": "1kb"
    },
    {
      "path": "lib/isPlainObject.js",
      "limit": "1kb"
    },
    {
      "path": "lib/map.js",
      "limit": "1kb"
    },
    {
      "path": "lib/mapValues.js",
      "limit": "1kb"
    },
    {
      "path": "lib/omit.js",
      "limit": "1kb"
    },
    {
      "path": "lib/omitBy.js",
      "limit": "1kb"
    },
    {
      "path": "lib/omitNull.js",
      "limit": "1kb"
    },
    {
      "path": "lib/pad.js",
      "limit": "1kb"
    },
    {
      "path": "lib/pick.js",
      "limit": "1kb"
    },
    {
      "path": "lib/pickBy.js",
      "limit": "1kb"
    },
    {
      "path": "lib/set.js",
      "limit": "1kb"
    },
    {
      "path": "lib/setProps.js",
      "limit": "1kb"
    },
    {
      "path": "lib/toPairs.js",
      "limit": "1kb"
    },
    {
      "path": "lib/types.js",
      "limit": "1kb"
    },
    {
      "path": "lib/uniq.js",
      "limit": "1kb"
    },
    {
      "path": "lib/uniqBy.js",
      "limit": "1kb"
    }
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isuvorov/fishdash.git"
  },
  "homepage": "https://github.com/isuvorov/fishdash",
  "bugs": "https://github.com/isuvorov/fishdash/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "utility",
    "library",
    "typescript",
    "esm",
    "es6",
    "functional",
    "programming",
    "utils",
    "helpers"
  ],
  "////": "/////////========================/////////========================/////////========================/////////"
}
